/* components/nav-title/nav-title.wxss */
.nav_title
{
  flex-direction: column;
  position: fixed;
  left: 0;
  top:0;
  background-color: #fff;
  z-index: 999;
  padding-bottom: 20rpx;
}
.flex_end
{
  display:flex;
  align-items: center;
  justify-content: flex-end;
}
.title
{
  width: 750rpx;
  box-sizing: border-box;
  /* padding-left: 57rpx; */
  /* padding-top: 83rpx; */
  display: flex;
  justify-content: center;
  align-items: center;
}
.f_42_fff
{
  font-size: 46rpx;
  color:#000;
}
.relative
{
  position: relative;
}
.arrow
{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width: 50rpx;
  height: 50rpx;
}
.address
{
  font-size: 30rpx;
  color:#000;
  margin-right: 10rpx;
  height: 50rpx;
}
.flex_start
{
  display: flex;
  justify-content: center;
  align-items: center;
}
.icon_box
{
  display: flex;
  justify-content: center;
  align-items: center;
}