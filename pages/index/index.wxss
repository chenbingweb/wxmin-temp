}
.exchange_btn
{
  width: 120rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color:#0a6fff;
  text-align: center;
  border-radius: 25rpx;
  border:2rpx solid #0a6fff;
}
.one_key
{
  position: fixed;
  right: 30rpx;
  bottom: 160rpx;
}
.order_nav
{
  background-color: rgba(0, 0, 0, 0.8);
}
.order_box
{
  position: absolute;
  width: 750rpx;
  left: 50%;
  top:50%;
  transform: translateX(-50%) translateY(-50%) scale(0.9);
  background-color: #fff;
  
}
.close
{
  position: absolute;
  top:-90rpx;
  right: 0rpx;
}
