.integral_exchange_box
{
  width: 750rpx;
  margin: 0 auto;
  box-sizing: border-box;
  padding:0 25rpx;
  border-bottom: 2rpx solid #dce6f5;
 
}

.integral_box
{
  width: 700rpx;
  margin: 0 auto;
  position: relative;
  height: 100rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
}
.integral_box::before
{
  content: "";
  position: absolute;
  left: 0;
  top:50%;
  transform: translateY(-50%);
  width: 10rpx;
  height: 24rpx;
  background-color: #0a6fff;
  border-radius: 5rpx;
}
.f_38_121212
{
  font-size: 38rpx;
  color:#121212;
}
.f_36_121212
{
  font-size: 36rpx;
  color:#121212;
}
.e_f_38_1a42a1
{
  font-size: 38rpx;
  color:#1a42a1;
  opacity: 0.2;
  margin-left: 17rpx;
}
.f_24_808080
{
  font-size: 24rpx;
  color:#808080;
}
.more
{
  width:80rpx;
  height: 50rpx;
  position: absolute;
  right: 0;
  top:50%;
  transform: translateY(-50%);
}
.integral_item_box
{
  width: 700rpx;
  margin: 0 auto;
  box-sizing: border-box;
}
.integal_item
{
  width: 700rpx;
  height: 140rpx;
  box-sizing: border-box;
  border-bottom:2rpx solid #dce6f5;
}

.img_box
{
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
